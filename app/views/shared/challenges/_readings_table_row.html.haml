%tr
  %td= reading.read_on.strftime("%-m/%-d")
  %td= link_to reading.chapter.book_and_chapter, reading_path(reading)
  %td{id: dom_id(reading)}
    - if membership
      = react_component 'ReadingButton', 
        { reading_id: reading.id, membership_id: membership.id, 
        membership_reading_id: membership_reading_id }
