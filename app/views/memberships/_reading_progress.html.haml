%h4
  Readings progress:
  = "#{@membership.membership_readings.read.count}/#{@membership.membership_readings.count}"
  = "(#{@membership.overall_progress_percentage}%)"
.progress.active
  .progress-bar{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "#{@membership.overall_progress_percentage}", role: "progressbar", style: "width: #{@membership.overall_progress_percentage}%;"}
    %span.sr-only #{@membership.overall_progress_percentage}% Complete
